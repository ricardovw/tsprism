{
  "name": "tsprism",
  "version": "1.1.0",
  "description": "a prism of type helpers and testing utilities for Typescript!",
  "keywords": [
    "Typescript",
    "Testing",
    "Types"
  ],
  "type": "module",
  "main": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.d.ts"
  ],
  "scripts": {
    "dr": "npm publish --dry-run",
    "pack": "rollup -c",
    "push": "npm run pack && npm publish --access public",
    "release": "npm run pack && npx changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardovw/tsprism.git"
  },
  "author": "ricardovw",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "rollup": "^4.27.3",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.6.3"
  }
}
